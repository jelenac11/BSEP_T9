package rules.admin.templates;
import com.tim9.bolnica.model.*;
import java.util.Date;

rule "Messages before and after rule .*database.* and .*database.* appears in 10 seconds 015240a3-5b01-460a-9f5e-0ca9f7a7049e"
   when
        $l1: Log(message matches ".*database.*", $t:timestamp)
        $l2: Log(new Date(timestamp.getTime() - 10*1000) < $t, new Date(timestamp.getTime()) > $t, message matches ".*database.*")
    then
    	System.out.println("Messages template rule");
        insert(new AdminAlarm(null, null, new Date(), "AJDE"));
   end


