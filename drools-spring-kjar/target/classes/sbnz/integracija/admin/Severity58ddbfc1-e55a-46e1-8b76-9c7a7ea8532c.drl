package rules.admin.templates;
import com.tim9.bolnica.model.*;
import com.tim9.bolnica.enums.*;
import java.util.Date;

rule "Severity rule INFORMATIONAL appears 5 times in 100000 seconds 58ddbfc1-e55a-46e1-8b76-9c7a7ea8532c"
    when
        Number(intValue >= 5) from accumulate(
           $l: Log(severity == LogSeverity.INFORMATIONAL) over window:time(100000s),
           count($l))
    then
        System.out.println("Severity template rule");
        insert(new AdminAlarm(null, null, new Date(), "SEVERITI"));
end


