package rules.doctor.templates;
import com.tim9.bolnica.model.*;
import java.util.Date;

rule "Pressure edbceff8-46d6-4783-89dc-49b920f1d351 "
    when
    	Message($patient: patientId, systolic < 200, systolic> 10, diastolic < 200, diastolic> 10, heartRate < 90, heartRate > 40)
    then
        System.out.println("Pressure template rule");
        insert(new DoctorAlarm(null, $patient, new Date(), "HELLO"));
   end


