package rules.doctor.templates;
import com.tim9.bolnica.model.*;
import java.util.Date;

rule "Low oxygen lever - below 90% 7d5755af-5438-461c-bc48-6a8205d838f3 "
    when
    	Message(oxygenLevel > 90, $patient: patientId)
    then
        System.out.println("Low oxygen level template rule");
        insert(new DoctorAlarm(null, $patient, new Date(), "Very low oxygen"));
   end


