package rules.doctor.templates;
import com.tim9.bolnica.model.*;
import java.util.Date;

rule "Low temperature below 37.0 2cdb911d-8cf1-47ce-8203-5af0642e7977 "
    when
    	Message(temperature < 37.0, $patient: patientId)
    then
        System.out.println("Low temperature template rule");
        insert(new DoctorAlarm(null, $patient, new Date(), "Very low temperature"));
   end


